<header class="navbar">
	<div class="navbar-inner">
		<div class="container-fluid">
			<a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
			</a>
			<%= link_to "Quota", dashboard_path, id: "logo", :class => "brand" %>
			<div class="nav-collapse">
				<ul class="nav pull-right">
					<% if signed_in? %>
						<li id="admin-menu" class="dropdown">
							<a style="font-size: 18px;" href="#" class="dropdown-toggle" data-toggle="dropdown">
								<i class="icon-cog"></i><b class="caret"></b>
							</a>
							<ul class="dropdown-menu">
								<li><%= link_to "Users", users_path %></li>
								<li><%= link_to "Reps", sales_reps_path %></li>
							</ul>
						</li>
						<li id="member-menu" class="dropdown">
							<a href="#" class="dropdown-toggle" data-toggle="dropdown">
								<%= current_user.name || current_user.email %> <b class="caret"></b>
							</a>
							<ul class="dropdown-menu">
								<li><%= link_to "Profile", current_user %></li>
								<li><%= link_to "Settings", '#' %></li>
								<li class="divider"></li>
								<li>
									<%= link_to "Sign out", signout_path, method: "delete" %>
								</li>
							</ul>
						</li>
					<% else %>
						<li><%= link_to "Sign in", signin_path %></li>
					<% end %>
				</ul>
			</div>
		</div>
	</div>
</header>